<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.devin.company_profile">
	<class name="com.devin.company_profile.entity.Index" table="d_index">
		<id name="id" length="5" type="java.lang.Long" column="id">
			<generator class="increment"></generator>
		</id>
		<property name="indexTitle" length="50" column="index_title" type="java.lang.String"></property>
		<property name="indexDescription" length="200" column="index_description" type="java.lang.String"></property>
		<property name="indexFile" length="200" column="index_file" type="java.lang.String"></property>
		<property name="aboutDescription" length="200" column="about_description" type="java.lang.String"></property>
		<property name="aboutFile" length="200" column="about_file" type="java.lang.String"></property>
		<property name="expertiseDescription" length="200" column="expertise_description" type="java.lang.String"></property>
		<property name="expertiseFile" length="200" column="expertise_file" type="java.lang.String"></property>
		<property name="createTime" column="create_time" type="java.util.Date"></property>
		<property name="updateTime" column="update_time" type="java.util.Date"></property>
		<property name="createUser" length="5" column="create_user" type="java.lang.Long"></property>
		<property name="updateUser" length="5" column="update_user" type="java.lang.Long"></property>
		<property name="status" length="1" type="java.lang.Integer"></property>
		
	</class>
	
</hibernate-mapping>